{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html"
      
    },

    "permissions": [
      "tabs",
      "activeTab"
    ],

    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],

    "chrome_url_overrides": {
      "newtab": "hello.html"
    }
  }